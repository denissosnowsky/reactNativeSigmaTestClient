{
  "name": "my-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "yarn run lint -- --fix",
    "lint:staged": "npx lint-staged --relative",
    "prettier": "prettier --check \"**/*.{tsx, ts}\"",
    "prettier:fix": "prettier --write \"**/*.{tsx, ts}\"",
    "prettier:staged": "pretty-quick --pattern \"**/*.{tsx, ts}\" --staged",
    "pre-commit-check": "yarn run prettier:staged && yarn run lint:staged",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --verbose --coverage",
    "test:coverageWithHtml": "jest --verbose --coverage && start ./coverage/lcov-report/index.html",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-test web 19006 cy:open",
    "cy:ci": "start-test web 19006 cy:run"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn run lint:fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-navigation/bottom-tabs": "^6.1.0",
    "@react-navigation/native": "^6.0.7",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "babel-jest": "26",
    "expo": "~44.0.0",
    "expo-constants": "~13.0.0",
    "expo-status-bar": "~1.2.0",
    "jest": "26",
    "jest-expo": "^44.0.1",
    "native-base": "^3.3.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-redux": "^7.2.6",
    "ts-jest": "26"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-native": "^9.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-react-native": "5.0.2",
    "cypress": "^9.3.1",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "expo-cli": "^5.0.3",
    "husky": "^7.0.0",
    "jest-react-native": "^18.0.0",
    "lint-staged": "^12.1.7",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.3.5"
  },
